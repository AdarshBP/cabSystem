<!DOCTYPE html>
<html lang="en">
<head>
  <title>Welcome Admin</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.css" integrity="sha256-46qynGAkLSFpVbEBog43gvNhfrOj+BmwXdxFgVK/Kvc=" crossorigin="anonymous" />
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">

  
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>


</head>
<body>
    
<nav class="navbar navbar-expand-sm bg-dark navbar-dark sticky-top">
  <!-- Brand -->
  <a class="navbar-brand" href="#">Logo</a>

  <!-- Links -->
  <ul class="navbar-nav">
    <li class="nav-item">
        <a class="nav-link active" href="/traveladmin"><i class="fas fa-users"></i></i> Approve Requests</a></li>
    <li  class="nav-item">
        <a class="nav-link" href="/MasterList"><i class="fas fa-pen-fancy"></i>  Master List</span></a></liclass="nav-item">
    <li  class="nav-item">
        <a class="nav-link" href="/dailyList"><i class="fas fa-pen-fancy"></i>  Today's List</span></a></liclass="nav-item">
    <li class="nav-item">
      <a class="nav-link"  href="/contactUs"><i class="fa fa-envelope"></i> Contact</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" href="/reportBug"><i class="fas fa-bug"></i> Report Bug</a>
    </li>
    <!-- Dropdown -->
    <li class="nav-item dropdown">
      <a class="nav-link dropdown-toggle" href="#" id="navbardrop" data-toggle="dropdown">
        <i class="fas fa-user-circle"></i> Account
      </a>
      <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
         <a class="dropdown-item" href="/profile">Profile</a>
         <a class="dropdown-item" href="/logOut">LogOut</a>
         <a class="dropdown-item" href="/resetPassword">ResetPassword</a>
      </div>
    </li>
  </ul>
</nav>
<br>
  
  <div class="container table-responsive">
    <table class="table table-hover">
      <thead class="thead-dark">
        <tr>
          <th colspan="7" style="text-align:center;"> Pending Approval</th>
        </tr>
      </thead>
        <thead class="table-primary">
        <tr>
            <th>EMP ID</th>
            <th>EMP NAME</th>
            <th>START_DATE</th>
            <th>END_DATE</th>
            <th>LOCATION</th>
            <th>REASON</th>
            <th>APPROVE</th>
        </tr>
    </thead>
    <tbody>
        <% data.pending_data.forEach((item)=>{ %>  
            <tr>
               <td><%= item.EMP_ID %></td>
               <td><%= item.EMP_NAME %></td>
               <td><%= item.START_DATE_TIME %></td> 
               <td ><%= item.END_DATE %> </td> 
               <td><%= item.START_LOCATION %></td> 
               <td><%= item.REASON %></td> 
               <td>
                  <form action='/admin/user-cab-approve' method="POST">
                    <button class="btn btn-success" type="submit">APPROVE</button>
                    <input type="hidden" value="<%=item.S_No %>" name="SNo">
                  </form>
                    <form action='/admin/user-cab-disapprove' method="POST">
                    <button class="btn btn-danger" type="submit">DECLINE</button>
                    <input type="hidden" value="<%=item.S_No %>" name="SNo">
                  </form>
                </td> 
            </tr>    
        <%});%>  
      </tbody>         
    </table>
  </div>

  <div class="container table-responsive">
    <table class="table table-hover ">
      <thead class="thead-dark">
        <tr>
          <th colspan="7" style="text-align:center;"> Approved List</th>
        </tr>
      </thead>
        <thead class="table-success">
        <tr>
            <th>EMP ID</th>
            <th>EMP NAME</th>
            <th>START_DATE</th>
            <th>END_DATE</th>
            <th>LOCATION</th>
            <th>REASON</th>
            <th>APPROVE</th>
        </tr>
    </thead>
    <tbody>
        <% data.TravelmangerApproved_data.forEach((item)=>{ %>  
            <tr>
               <td><%= item.EMP_ID %></td>
               <td><%= item.EMP_NAME %></td>
               <td><%= item.START_DATE_TIME %></td> 
               <td><%= item.END_DATE %></td> 
               <td><%= item.START_LOCATION %></td> 
               <td><%= item.REASON %></td> 
               <td>
                    <button class="btn btn-success">APPROVED</button>
                    <input type="hidden" value="<%=item.S_No %>" name="SNo">
               </td> 
            </tr>    
        <%});%>  
      </tbody>         
    </table>
  </div>

  <div class="container table-responsive">
    <table class="table table-hover ">
      <thead class="thead-dark">
        <tr>
          <th colspan="7" style="text-align:center;"> DisApproved List</th>
        </tr>
      </thead>
        <thead class="table-danger">
        <tr>
            <th>EMP ID</th>
            <th>EMP NAME</th>
            <th>START_DATE</th>
            <th>END_DATE</th>
            <th>LOCATION</th>
            <th>REASON</th>
            <th>APPROVE</th>
        </tr>
    </thead>
    <tbody>
        <% data.TravelmangerDisapproved_data.forEach((item)=>{ %>  
            <tr>             
               <td><%= item.EMP_ID %></td>
               <td><%= item.EMP_NAME %></td>
               <td><%= item.START_DATE_TIME %></td> 
               <td><%= item.END_DATE.substring %></td> 
               <td><%= item.START_LOCATION %></td> 
               <td><%= item.REASON %></td> 
               <td>
                  <form action='/admin/user-cab-approve' method="POST">
                    <button class="btn btn-success" type="submit">APPROVE</button>
                    <input type="hidden" value="<%=item.S_No %>" name="SNo">
                  </form>+
               </td> 
            </tr>    
        <%});%>  
      </tbody>         
    </table>
  </div>

</body>
</html>
