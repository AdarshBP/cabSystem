<!DOCTYPE html>
<html lang="en">
<head>
  <title>Welcome Admin</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.css" integrity="sha256-46qynGAkLSFpVbEBog43gvNhfrOj+BmwXdxFgVK/Kvc=" crossorigin="anonymous" />
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">

  
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>
  <script src="jquery.tableToExcel.js"></script>
  <script>
    function writeDataToExcel(){
      $('.tableData').tblToExcel();
      }
  </script>

</head>
<body>
    
<nav class="navbar navbar-expand-sm bg-dark navbar-dark sticky-top">
  <!-- Brand -->
  <a class="navbar-brand" href="#">Logo</a>

  <!-- Links -->
  <ul class="navbar-nav">
    <li class="nav-item">
        <a class="nav-link active" href="/traveladmin"><i class="fas fa-users"></i></i> Approve Requests</a></li>
    <li  class="nav-item">
        <a class="nav-link" href="/MasterList"><i class="fas fa-pen-fancy"></i> Master List</span></a></liclass="nav-item">
    <li  class="nav-item">
        <a class="nav-link" href="/dailyList"><i class="fas fa-pen-fancy"></i> Today's List</span></a></liclass="nav-item">
    <li class="nav-item">
      <a class="nav-link"  href="/contactUs"><i class="fa fa-envelope"></i> Contact</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" href="/reportBug"><i class="fas fa-bug"></i> Report Bug</a>
    </li>
    <!-- Dropdown -->
    <li class="nav-item dropdown">
      <a class="nav-link dropdown-toggle" href="#" id="navbardrop" data-toggle="dropdown">
        <i class="fas fa-user-circle"></i> Account
      </a>
      <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
         <a class="dropdown-item" href="/profile">Profile</a>
         <a class="dropdown-item" href="/logOut">LogOut</a>
         <a class="dropdown-item" href="/resetPassword">ResetPassword</a>
      </div>
    </li>
  </ul>
</nav>
<br>

  <div class="container table-responsive ">
    <button onclick="writeDataToExcel();" class="btn btn-primary" >Export to Excel </button>
    <div class="tableData">
    <table class="table table-hover mytable">
      <thead class="thead-dark">
        <tr>
          <th colspan="7" style="text-align:center;">Special Pass</th>
      </tr>
      </thead>
        <thead class="thead-dark ">           
        <tr>
            <th>Cab ID</th>
            <th>EMP ID</th>
            <th>EMP NAME</th>
            <th>START_DATE</th>
            <th>END_DATE</th>
            <th>LOCATION</th>
            <th>REASON</th>
        </tr>
        </thead>
    <tbody>
        <% data.specialList.forEach((item)=>{ %>  
            <tr>
               <td><%= item.S_No %></td>
               <td><%= item.EMP_ID %></td>
               <td><%= item.EMP_NAME %></td>
               <td><%= item.START_DATE_TIME %></td> 
               <td ><%= item.END_DATE %> </td> 
               <td><%= item.START_LOCATION %></td> 
               <td><%= item.REASON %></td> 
            </tr>    
        <%});%>  
       </tbody>         
    </table>
  <!-- </div>

  <div class="container table-responsive"> -->
    <table class="table table-hover"> 
        <thead class="thead-dark">
            <tr>
                <th colspan="7" style="text-align:center;">Daily Pass</th>
            </tr>    
        <tr>
            <th>Cab ID</th>
            <th>EMP ID</th>
            <th>EMP NAME</th>
            <th>START_DATE</th>
            <th>END_DATE</th>
            <th>LOCATION</th>
            <th>REASON</th>
        </tr>
        </thead>
    <tbody>
        <% data.dailyList.forEach((item)=>{ %>  
            <tr>
               <td><%= item.S_No %></td>
               <td><%= item.EMP_ID %></td>
               <td><%= item.EMP_NAME %></td>
               <td><%= item.START_DATE_TIME %></td> 
               <td ><%= item.END_DATE %> </td> 
               <td><%= item.START_LOCATION %></td> 
               <td><%= item.REASON %></td> 
            </tr>    
        <%});%>  
      </tbody>         
    </table>
    </div>
  </div>

</body>
</html>
